    "baseUrl": "./src",
    "paths": {
      "@api": [
        "api"
      ],
      "@api/*": [
        "api/*"
      ],
      "@assets": [
        "assets"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components": [
